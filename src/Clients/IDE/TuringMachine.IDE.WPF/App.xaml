<Application x:Class="TuringMachine.IDE.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TuringMachine.IDE.WPF"
             xmlns:vm="clr-namespace:TuringMachine.IDE.ViewModels;assembly=TuringMachine.IDE"
             xmlns:converters="clr-namespace:TuringMachine.IDE.WPF.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <vm:ViewModelLocator x:Key="Locator" />
        <converters:AlphabetToStringConverter x:Key="AlphabetToStringConverter" />
        <converters:OptionalCharToStringConverter x:Key="OptionalCharToStringConverter" />
    </Application.Resources>
</Application>
